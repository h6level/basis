{
  "name": "one",
  "version": "0.0.1",
  "description": "webpack5 for react + typescript template.",
  "main": "index.ts",
  "scripts": {
    "prepare": "npx husky install",
    "dev": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack server --config ./config/webpack.dev.ts",
    "build": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./config/webpack.prod.ts",
    "start": "pnpm run dev",
    "lint": "cross-env NODE_ENV=development eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "stylelint": "stylelint '**/*.{css,scss}'"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=development eslint . --cache"
    ]
  },
  "keywords": [
    "react",
    "typescript",
    "webpack5"
  ],
  "author": "h5lianfu",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/register": "^7.16.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "cssnano": "^5.0.12",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-react": "^7.27.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.2",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.4.4",
    "postcss-loader": "^6.2.1",
    "postcss-scss": "^4.0.2",
    "react-dev-utils": "^11.0.4",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.44.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.1.0",
    "stylelint-scss": "^4.0.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@fontsource/open-sans": "^4.5.2",
    "@fontsource/ubuntu": "^4.5.0",
    "react": "^18.0.0-beta-24dd07bd2-20211208",
    "react-dom": "^18.0.0-beta-24dd07bd2-20211208"
  }
}
